{
  "name": "todo-apps-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "nyc mocha --timeout 5000  \"test/**/*.test.js\" --recursive --exit ",
    "coverage": "nyc report --reporter=lcov",
    "test-cover": "npm run test && npm run coverage"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.js?$",
    "testResultsProcessor": "jest-junit"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cucumber": "^4.0.0",
    "jest": "^25.0.0",
    "jest-junit": "^3.4.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^3.4.2"
  }
}
